{
  "framework": "vite",
  "buildCommand": "npm ci --prefer-offline --no-audit --progress=false && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm ci --prefer-offline --no-audit --progress=false",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
